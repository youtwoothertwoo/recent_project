	border-bottom: 10rpx solid #666666;
	width: 60rpx;
	height: 60rpx;
	-webkit-animation: spin-data-v-eb4cbd66 1.4s linear infinite;
	animation: spin-data-v-eb4cbd66 1.4s linear infinite;
}
.loading-img.data-v-eb4cbd66{
	width: 60rpx;
	height: 60rpx;
}
@-webkit-keyframes spin-data-v-eb4cbd66 {
0% {
		-webkit-transform: rotate(0deg);
}
100% {
		-webkit-transform: rotate(360deg);
}
}
@keyframes spin-data-v-eb4cbd66 {
0% {
		-webkit-transform: rotate(0deg);
		        transform: rotate(0deg);
}
100% {
		-webkit-transform: rotate(360deg);
		        transform: rotate(360deg);
}
}

