<view class="qrcode"><swiper class="swiper" current="{{current}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{qrcodes}}" wx:for-item="url" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="{{url}}"></image></view></swiper-item></block></swiper><view class="dots"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="value"><view class="{{['dot',(index===current)?'active':'']}}"></view></block></view><view class="btns-wrap"><button class="btn-red" type="default" data-event-opts="{{[['tap',[['savePosterImg',['$event']]]]]}}" bindtap="__e">保存图片</button></view></view>