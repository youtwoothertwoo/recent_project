<block wx:if="{{!loadding}}"><view class="{{['index-agent','o-h',$root.m0]}}" data-theme="{{$root.m1}}"><view class="banner d-c-c d-c"><image src="{{top_background}}" mode="aspectFill"></image></view><block wx:if="{{is_agent&&isData}}"><view class="info-top d-s-c"><view class="info-ava"><image class="info-avatar" src="{{user.avatarUrl}}" mode="aspectFill"></image><block wx:if="{{agent.grade_id>0}}"><view class="info-grade">{{agent.grade.name}}</view></block></view><view class="white d-c d-c-s flex-1 pr"><view class="mb20 f30 fb">{{user.nickName}}</view><view class="f22">{{(info_words.index.words.referee.value||'')+":"+(agent.referee?agent.referee.nickName:'平台')}}</view></view></view><view class="agent-wrap pr m-0-20"><view class="d-s-c p-30-0 top_dash"><view class="flex-1 d-c-c d-c"><view class="redF6"><text class="f36 fb">{{agent.money}}</text></view><view class="pt20 f24 gray6">{{info_words.index.words.money.value}}</view></view><view class="flex-1 d-c-c d-c"><view><text class="f36 fb">{{agent.freeze_money}}</text></view><view class="pt20 f24 gray6">{{info_words.index.words.freeze_money.value}}</view></view><view class="flex-1 d-c-c d-c"><view><text class="f36 fb">{{agent.total_money}}</text></view><view class="pt20 f24 gray6">{{info_words.index.words.total_money.value}}</view></view></view><view class="d-c-c pt30"><button class="btn-gcred flex-1" type="primary" data-event-opts="{{[['tap',[['gotoCash',['$event']]]]]}}" bindtap="__e">{{info_words.index.words.cash.value}}</button></view></view><view class="agent-wrap m-0-20 p30 d-s-c f-w mt20 bg-white"><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/cash/list/list']]]]]}}" class="d-c-c d-c flex-1" bindtap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-zijinmingxi.png" mode></image></view><text class="f24 gray6 mt20">{{info_words.cash_list.title.value}}</text></view><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/order/order']]]]]}}" class="d-c-c d-c flex-1" bindtap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-fenxiaodingdan.png" mode></image></view><text class="f24 gray6 mt20">{{info_words.order.title.value}}</text></view><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/team/team']]]]]}}" class="d-c-c d-c flex-1" bindtap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-tuandui.png" mode></image></view><text class="f24 gray6 mt20">{{info_words.team.title.value}}</text></view><view data-event-opts="{{[['tap',[['gotoPage',['/pages/agent/qrcode/qrcode']]]]]}}" class="d-c-c d-c flex-1" bindtap="__e"><view><image class="agent_index_img" src="../../../static/icon/icon-erweima.png" mode></image></view><text class="f24 gray6 mt20">{{info_words.qrcode.title.value}}</text></view></view></block><block wx:if="{{!is_agent&&isData}}"><view class="no-agent" style="margin-top:348rpx;"><image class="no-agent-image" src="/static/agent_no.png" mode></image><view class="mt50 p-0-20 gray9 f30 tc">{{info_words.index.words.not_agent.value}}</view><view class="p30 mt30"><button class="btn-gcred" type="primary" data-event-opts="{{[['tap',[['applyagent',['$event']]]]]}}" bindtap="__e">{{info_words.index.words.apply_now.value}}</button></view></view></block></view></block>